{
	"root": true,
	"env": {
		"node": true
	},
	"extends": [
		"plugin:vue/vue3-recommended",
		"eslint:recommended"
	],
	"parserOptions": {
		"parser": "@babel/eslint-parser"
	},
	"rules": {
		"indent": ["warn", "tab"],
		"quotes": ["warn", "single"],
		"semi": ["warn", "always"],
		"vue/html-indent": ["warn", "tab"],
		"vue/max-attributes-per-line": "off"
	},
	"globals": {
		"defineProps": "readonly",
		"defineEmits": "readonly"
	}
}
